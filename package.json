{
  "name": "hypertesting",
  "version": "1.0.7",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "init-ts-lib": "dist/index.js"
  },
  "scripts": {
    "start": "tsc -w",
    "watch": "jest --watch",
    "test": "jest",
    "test:bin": "node dist/index.js",
    "release": "yarn build && git commit -am 'build'; yarn version && git push --tags && git push origin master",
    "build": "tsc"
  },
  "dependencies": {
    "ejs": "^2.6.1",
    "js-yaml": "^3.13.0",
    "lodash": "^4.17.11",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "@types/ejs": "^2.6.3",
    "@types/jest": "^24.0.11",
    "@types/js-yaml": "^3.12.0",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.11.4",
    "@types/supertest": "^2.0.7",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.4000",
    "zeroconf-typescript-eslint": "^1.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(src/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
